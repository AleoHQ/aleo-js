{
  "source": {
    "include": ["src/account.ts", "README.md", "package.json"],
    "includePattern": "\\.(jsx|js|ts|tsx)$"
  },
  "tags": {
    "allowUnknownTags": ["optional"]
  },
  "plugins": [
      "node_modules/better-docs/typescript"
  ],
  "opts": {
    "encoding": "utf8",
    "readme": "./README.md",
    "destination": "docs/",
    "recurse": true,
    "verbose": true,
    "template": "./node_modules/clean-jsdoc-theme",
    "theme_opts": {
      "default_theme": "dark"
    }
  },
  "markdown": {
    "hardwrap": false,
    "idInHeadings": true
  }
}
